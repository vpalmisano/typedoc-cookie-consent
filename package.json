{
  "name": "@vpalmisano/typedoc-cookie-consent",
  "version": "0.0.2",
  "author": "Vittorio Palmisano",
  "description": "A TypeDoc plugin to add a cookie consent banner to your documentation.",
  "license": "MIT",
  "keywords": [
    "typedoc",
    "plugin",
    "typedocplugin",
    "typedoc-plugin"
  ],
  "bugs": {
    "url": "https://github.com/vpalmisano/typedoc-cookie-consent/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpalmisano/typedoc-cookie-consent.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "npm run clear && npm run format && npm run lint && tsc",
    "clear": "rm -rf ./dist",
    "lint": "eslint src/*.tsx",
    "lint:fix": "eslint src/*.tsx --fix",
    "format": "prettier --write ./src/*.tsx",
    "prepare": "npm run build"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "typedoc": "^0.28.5",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.35.0",
    "vanilla-cookieconsent": "^3.1.0"
  }
}
